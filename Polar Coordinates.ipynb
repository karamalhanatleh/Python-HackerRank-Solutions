   "source": [
    "import cmath\n",
    "\n",
    "# Read input\n",
    "complex_number = complex(input().strip())\n",
    "\n",
    "# Calculate modulus and phase\n",
    "modulus = abs(complex_number)\n",
    "phase_angle = cmath.phase(complex_number)\n",
    "\n",
    "# Output results with 3 decimal places\n",
    "print(f\"{modulus:.3f}\")\n",
    "print(f\"{phase_angle:.3f}\")"
   ]
